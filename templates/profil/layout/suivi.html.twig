<h4>Utilisateurs Suivi</h4>
{% for suivi in suivis %}
    <a id="suivi-{{suivi.id}}" class="link-secondary text-decoration-none suivis" href="{{ path('app_user_detail', { slug: suivi.slug }) }}"><img class="img-profil" src="{{ suivi.image }}" alt="{{ suivi.nom }}" width="50px" height="50px"/>
        {{ suivi.nom }}
        {{ suivi.prenom }}</a><br/>
{% endfor %}